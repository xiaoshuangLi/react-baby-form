  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoshuangLi/react-baby-form"
  },
  "scripts": {
    "start": "npm run production",
    "dev": "npm-run-all development",
    "development": "npm-run-all --parallel development:client:build development:server:build development:services",
    "development:client:build": "babel-node ./example/webpack/webpack-dev-server.js",
    "development:server:build": "webpack --mode development --config ./example/webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development:server": "nodemon ./example/server/index.entry.js --watch ./example/server --watch ./example/public/server",
    "development:services": "npm-run-all --parallel development:server",
    "production": "npm-run-all production:client:build production:server:build production:services",
    "production:client:build": "webpack --mode production --colors --display-error-details --config ./example/webpack/webpack.config.client.production.babel.js",
    "production:server:build": "webpack --mode production --colors --display-error-details --config ./example/webpack/webpack.config.server.production.babel.js",
    "production:server": "better-npm-run production:server",
    "production:services": "npm-run-all --parallel production:server",
    "build": "rm -rf lib && webpack --config webpack/webpack.config.umd.js &&  webpack --config webpack/webpack.config.prd.js",
    "prepublishOnly": "npm run build && rm ./lib/*.html"
  },
  "betterScripts": {
    "production:server": {
      "command": "node ./example/server/index.entry.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "react"
  ],
  "author": "Xiaoshuang",
  "homepage": "https://xiaoshuangli.github.io/web-just-for-fun/",
